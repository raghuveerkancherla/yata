{
    "env": {
        "node": true,
        "browser": true,
        "jasmine": true,
        "jquery": true,
        "es6": true
    },
    "globals": {
        "_": true,
        "Rollbar": true,
        "google": true,
        "gapi": true
    },
    "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "blockBindings": true,
        "classes": true
    },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "rules": {
        "no-alert": 1,
        "no-array-constructor": 0,
        "no-bitwise": 2,
        "no-caller": 1,
        "no-catch-shadow": 2,
        "no-class-assign": 2,
        "no-cond-assign": 2,
        "no-console": 2,
        "no-const-assign": 2,
        "no-constant-condition": 2,
        "no-continue": 0,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-delete-var": 2,
        "no-div-regex": 0,
        "no-dupe-keys": 2,
        "no-dupe-args": 2,
        "no-duplicate-case": 2,
        "no-else-return": 1,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-empty-label": 0,
        "no-eq-null": 1,
        "no-eval": 2,
        "no-ex-assign": 2,
        "no-extend-native": 1,
        "no-extra-bind": 0,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": 0,
        "no-extra-semi": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 0,
        "no-func-assign": 2,
        "no-inner-declarations": [2, "functions"],
        "no-invalid-regexp": 2,
        "no-invalid-this": 0,
        "no-irregular-whitespace": 2,
        "no-label-var": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-lonely-if": 2,
        "no-loop-func": 2,
        "no-mixed-requires": [1, false],
        "no-mixed-spaces-and-tabs": [2, false],
        "linebreak-style": [0, "unix"],
        "no-multi-spaces": 0,
        "no-multi-str": 0,
        "no-multiple-empty-lines": [0, {"max": 2}],
        "no-native-reassign": 1,
        "no-negated-in-lhs": 2,
        "no-nested-ternary": 1,
        "no-new": 0,
        "no-new-func": 2,
        "no-new-object": 1,
        "no-obj-calls": 2,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-script-url": 1,
        "no-self-compare": 2,
        "no-sequences": 1,
        "no-shadow": 1,
        "no-shadow-restricted-names": 1,
        "no-sparse-arrays": 2,
        "no-undefined": 2,
        "no-undef": 2,
        "no-unexpected-multiline": 1,
        "no-unreachable": 2,
        "no-unused-vars": [2, {"vars": "all", "args": "none"}],
        "no-useless-call": 0,
        "no-void": 1,

        "array-bracket-spacing": [0, "never"],
        "arrow-parens": 0,
        "arrow-spacing": 0,
        "accessor-pairs": 0,
        "block-scoped-var": 0,
        "block-spacing": 0,
        "brace-style": [0, "1tbs"],
        "camelcase": 0,
        "comma-dangle": [2, "never"],
        "comma-spacing": 0,
        "comma-style": 0,
        "complexity": [0, 11],
        "computed-property-spacing": [0, "never"],
        "consistent-return": 0,
        "consistent-this": [0, "that"],
        "constructor-super": 0,
        "curly": [0, "all"],
        "default-case": 0,
        "dot-location": 0,
        "dot-notation": [0, { "allowKeywords": true }],
        "eol-last": 0,
        "eqeqeq": 0,
        "func-names": 0,
        "func-style": [0, "declaration"],
        "generator-star-spacing": 0,
        "guard-for-in": 0,
        "handle-callback-err": 0,
        "id-length": 0,
        "indent": 0,
        "init-declarations": 0,
        "key-spacing": [0, { "beforeColon": false, "afterColon": true }],
        "lines-around-comment": 0,
        "max-depth": [1, 3],
        "max-len": [1, 120, 4, {"ignoreUrls": true}],
        "max-nested-callbacks": [1, 2],
        "new-cap": 0,
        "new-parens": 1,
        "newline-after-var": 0,
        "operator-assignment": [2, "always"],
        "operator-linebreak": 0,
        "one-var": [2, "never"],
        "quote-props": 0,
        "quotes": [1, "single",  "avoid-escape"],
        "require-yield": 0,
        "semi": [2, "always"],
        "space-before-blocks": [1, "always"],
        "space-after-keywords": [1, "always"],
        "strict": 0,
        "use-isnan": 2,
        "valid-jsdoc": 0,
        "valid-typeof": 2,
        "vars-on-top": 1,
        "wrap-iife": 1,
        "wrap-regex": 1,
        "yoda": [0, "never"],
        "jsx-quotes": 1,
        "react/jsx-boolean-value": [1, "always"],
        "react/jsx-curly-spacing": 1,
        "react/jsx-max-props-per-line": 1,
        "react/jsx-no-duplicate-props": 2,
        "react/jsx-no-undef": 2,
        "react/jsx-uses-react": 1,
        "react/jsx-uses-vars": 1,
        "react/no-danger": 1,
        "react/no-did-mount-set-state": 1,
        "react/no-did-update-set-state": 2,
        "react/no-multi-comp": 0,
        "react/no-unknown-property": 2,
        "react/prop-types": 2,
        "react/react-in-jsx-scope": 1,
        "react/require-extension": 1,
        "react/self-closing-comp": 1,
        "react/wrap-multilines": 1,
        "react/sort-comp": [2, {
          "order": [
            "lifecycle",
            "/^on.+$/",
            "/^(get|set)(?!(InitialState$|DefaultProps$|ChildContext$)).+$/",
            "everything-else",
            "/^render.+$/",
            "render"
          ]
        }]
    }
}
